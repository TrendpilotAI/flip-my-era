# API Keys
VITE_GROQ_API_KEY=your-groq-api-key
VITE_CLOUDFLARE_SITE_KEY=your-cloudflare-site-key
VITE_CLOUDFLARE_SECRET_KEY=your-cloudflare-secret-key
VITE_OPENAI_API_KEY=your-openai-api-key
RUNWARE_API_KEY=your-runware-api-key
VITE_RUNWARE_PROXY_URL=/api/functions/runware-proxy

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-supabase-publishable-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
VITE_SUPABASE_JWT_SECRET=your-supabase-jwt-secret

# Application Configuration
VITE_APP_URL=http://localhost:8080
VITE_APP_ENV=development

# Sentry Error Tracking
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_SENTRY_SEND_DEFAULT_PII=false

# OpenTelemetry (OTLP) Configuration for Sentry
VITE_OTLP_ENABLED=false
VITE_OTLP_ENDPOINT=https://<your-org-host>.sentry.io/api/<your-project-id>/integration//otlp
VITE_SENTRY_AUTH_TOKEN=your-sentry-auth-token-optional

# Auth is handled by Supabase Auth (no additional keys needed)

